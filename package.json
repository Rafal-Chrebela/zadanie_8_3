{
  "name": "learning-git-3",
  "version": "1.0.0",
  "description": "https://rafal-chrebela.github.io/learning-git-3/",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:html": "globstar nu-html-checker *.html",
    "test:js": "jshint js",
    "init-project": "npm install && npm-run-all init:*",
    "init:dirs": "mkdir sass css vendor images js",
    "init:files": "touch README.md index.html sass/style.scss js/script.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm-run-all build:* test",
    "build:clean": "rimraf css/*",
    "build:sass": "node-sass --output-style compact -o css sass",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "build-dev:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "watch": "npm-run-all build -p watch:*",
    "watch:sassprefixer": "onchange -d 100 sass/* -- node-sass --output-style expanded --source-map true -o css sass -- autoprefixer-cli -o css/style.css",
    "watch:browsersync": "browser-sync start --server --files css/*.css *.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rafal-Chrebela/learning-git-3.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rafal-Chrebela/learning-git-3/issues"
  },
  "homepage": "https://github.com/Rafal-Chrebela/learning-git-3#readme",
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.23.6",
    "browsersync": "0.0.1-security",
    "globstar": "^1.0.0",
    "jshint": "^2.9.5",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "nu-html-checker": "^0.1.0",
    "onchange": "^3.3.0"
  },
  "dependencies": {
    "mkdirp": "^0.5.1"
  }
}
